<?xml version="1.0" encoding="utf-8"?><resources>    <style name="AppTheme" parent="android:Theme.Material.Light.DarkActionBar">        <!-- Main theme colors -->        <!--   your app branding color for the app bar -->        <item name="android:colorPrimary">@color/primary</item>        <!--   darker variant for the status bar and contextual app bars -->        <item name="android:colorPrimaryDark">@color/primary_dark</item>        <!--   theme UI controls like checkboxes and text fields -->        <item name="android:colorAccent">@color/accent</item>        <!--main windows background color-->        <item name="android:windowBackground">@color/primary_dark</item>        <!--actionbar overlay-->        <item name="android:windowActionBarOverlay">true</item>    </style></resources>